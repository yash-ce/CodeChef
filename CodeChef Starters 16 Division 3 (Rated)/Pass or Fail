//https://www.codechef.com/START16C/problems/PASSORFAIL

#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	while(n--){
	    int N, X, P;
	    cin>>N>>X>>P;
	    
	    if((3*X)-(N-X) >= P){
	        cout<<"PASS"<<endl;
	    }
	    else{
	        cout<<"FAIL"<<endl;
	    }
	}
	return 0;
}
