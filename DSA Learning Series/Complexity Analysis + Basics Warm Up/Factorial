//  https://www.codechef.com/LRNDSA01/problems/FCTRL

#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int zeros = 0;
	    int c = 5;
	    
	    while((n/c) > 0){
	        zeros = zeros + (n/c);
	        c = c*5;
	    }
	    
	    cout<<zeros<<endl;
	}
	return 0;
}
