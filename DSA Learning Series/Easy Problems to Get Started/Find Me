//https://www.codechef.com/CCSTART2/problems/FINDMELI

#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int n, k;
	cin>>n>>k;
	int a[n];
	for(int i=0;i<n;i++){
	    cin>>a[i];
	}
	int i=0;
	for(i=0;i<n;i++){
	    if(a[i] == k){
	     cout<<1<<endl;
	     break;
	    }
	}
	if(i == n){
	    cout<<-1<<endl;
	}
	
	return 0;
}
