//https://www.codechef.com/CCSTART2/problems/SECLAR

#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int a,b,c;
	cin>>a>>b>>c;
	if((a > b && a < c) || (a > c && a < b)){
	    cout<<a<<endl;
	}
	else if((b > c && b < a) || (b > a && b < c)){
	    cout<<b<<endl;
	}
	else{
	    cout<<c<<endl;
	}
	return 0;
}
