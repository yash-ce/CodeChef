//https://www.codechef.com/CCSTART2/problems/REVSTRPT


#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	for(int i=n-1;i>=0;i--){
	    for(int j=0;j<i;j++){
	        cout<<" ";
	    }
	   // cout<<endl;
	    for(int k=n-i;k>0;k--){
	        cout<<"*";
	    }
	    cout<<endl;
	    
	}
	return 0;
}
