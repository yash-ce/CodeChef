#include <iostream>
using namespace std;

int main() {
	// your code goes here
	long long int t;
	cin>>t;
	while(t--){
	    long long int n, a;
	    cin>>n>>a;
	    
	    if(n == 1){
	        if(a%2 == 0){
	             cout<<"even"<<endl;
	        }
	        else{
	            cout<<"odd"<<endl;
	        }
	        continue;
	    }
	    if(a%2 == 1){
	        if(n%2 == 0){
	            cout<<"even"<<endl;
	        }
	        else{
	            cout<<"odd"<<endl;
	        }
	    }
	    else{
	        cout<<"Impossible"<<endl;
	    }
	}
	return 0;
}
