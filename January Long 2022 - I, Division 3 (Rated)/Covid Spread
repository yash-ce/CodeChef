t = int(input())

for i in range(t):
    n, d = input().split()
    n = int(n)
    d = int(d)
    s = 1
    for i in range(1, d+1):
        if i <= 10:
            s = s*2
            if(s > n):
                s = n
                break
        else:
            s = s * 3
            if(s > n):
                s = n
                break
    print(s)
    
