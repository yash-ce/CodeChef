#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    float t1, t2, r1, r2;
	    cin>>t1>>t2>>r1>>r2;
	   // cout<<(t1*t1)/(r1*r1*r1)<<"  "<<(t2*t2)/(r2*r2*r2)<<endl;
	    if((t1*t1)/(r1*r1*r1) == (t2*t2)/(r2*r2*r2)){
	        cout<<"YES"<<endl;
	    }
	    else{
	        cout<<"NO"<<endl;
	    }
	}
	return 0;
}
