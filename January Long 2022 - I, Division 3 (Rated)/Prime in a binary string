#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int c = 0, p =-1;
	    string s;
	    cin>>s;
	    int n = s.length();
	    if(n == 1){
	        cout<<"NO"<<endl;
	        continue;
	        
	    }
	    int flag = 0;
	    for(int i=0;i<n-1;i++){
	        if(s[i] == '1'){
	            flag = 1;
	            cout<<"YES"<<endl;
	            break;
	        }
	    }
	    if(flag == 0){
	    cout<<"NO"<<endl;
	    }
	}
	return 0;
}
