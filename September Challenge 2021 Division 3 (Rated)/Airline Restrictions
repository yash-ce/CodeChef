// https://www.codechef.com/SEPT21C/problems/AIRLINE

#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	while(n--){
	    int a[5];
	    for(int i=0;i<5;i++){
	        cin>>a[i];
	    }
	    if(a[0]+a[1] <= a[3] && a[2] <= a[4]){
	        cout<<"YES"<<endl;
	   }
	
	    else if(a[0]+a[2] <= a[3] && a[1] <= a[4]){
	        cout<<"YES"<<endl;
	   }
	   else if(a[1]+a[2] <= a[3] && a[0] <= a[4]){
	        cout<<"YES"<<endl;
	   }
	   else{
	       cout<<"NO"<<endl;
	   }
	    
	    
	}
	return 0;
}
